<!DOCTYPE portlet-app PUBLIC
   "-//JBoss Portal//DTD JBoss Portlet 2.6//EN"
   "http://www.jboss.org/portal/dtd/jboss-portlet_2_6.dtd">



<portlet-app>
	<portlet>
		<portlet-name>toutatice-application-ficheapplication-portailPortlet</portlet-name>

		<transaction>
			<trans-attribute>Required</trans-attribute>
		</transaction>
		<header-content>
			<link rel="stylesheet" type="text/css" href="/css/ficheApplicationStyle.css"
				title="" media="screen" />
			<link rel="stylesheet" type="text/css" href="/css/menu_onglet.css"
				title="" media="screen" />
		</header-content>

	</portlet>

	<service>
		<service-name>PortalUrlFactory</service-name>
		<service-class>fr.toutatice.portail.api.urls.IPortalUrlFactory</service-class>
		<service-ref>pia:service=UrlFactory</service-ref>
	</service>


</portlet-app>

	
